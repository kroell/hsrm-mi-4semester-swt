package playground;

/**
 * Wall.java
 * 
 * Klasse zum Erstellen einer Wand, die innerhalb einer Area angezeigt wird
 * 
 * @author Soeren Kroell, Stephanie Scholl, Mario Sigel, Ersin Yilmiz
 */
public class Wall {

	private String file;
	private boolean transparency;
	private boolean solid;
	
	public Wall(String file, boolean transparency, boolean solid){
		this.file=file;
		this.transparency=transparency;
		this.solid=solid;
	}
	
	public String getFile(){
		return file;
	}
	
	public boolean isTransparency(){
		return transparency;
	}
	
	public boolean isSolid(){
		return solid;
	}
}
